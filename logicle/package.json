{
  "name": "logicle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:sqlite": "npm run push:sqlite && DATABASE_URL=file:///tmp/logicle.sqlite npm run dev",
    "dev:postgres": "npm run push:postgres && DATABASE_URL=postgresql://admin:admin@localhost:5432/logicle npm run dev",
    "build": "next build",
    "start": "next start",
    "start:sqlite": "npm run push:sqlite && DATABASE_URL=file:///tmp/logicle.sqlite npm run start",
    "start:postgres": "npm run push:postgres && DATABASE_URL=postgresql://admin:admin@localhost:5432/logicle npm run start",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --env=jsdom",
    "build-ci": "next build",
    "push:sqlite": "DATABASE_URL=file:///tmp/logicle.sqlite npx ts-node db/migrate.ts",
    "push:postgres": "DATABASE_URL=postgresql://admin:admin@localhost:5432/logicle npx ts-node db/migrate.ts",
    "generate-kysely": "npx kysely-codegen --url /tmp/logicle.sqlite --out-file db/types.ts"
  },
