  "scripts": {
    "dev": "tsx server.ts --turbo",
    "dev:sqlite": "DATABASE_URL=file://${PWD}/tmp/logicle.sqlite npm run dev",
    "dev:postgres": "DATABASE_URL=postgresql://admin:admin@localhost:5432/logicle npm run dev",
    "build": "DATABASE_URL=memory: next build --turbopack && tsup",
    "start": "NODE_ENV=production node dist-server/server.js",
    "start:sqlite": "DATABASE_URL=file://${PWD}/tmp/logicle.sqlite npm run start",
    "start:postgres": "DATABASE_URL=postgresql://admin:admin@localhost:5432/logicle npm run start",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "lint": "biome check",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --env=jsdom",
    "build-ci": "next build",
    "generate": "DATABASE_URL=memory: FILE_STORAGE_LOCATION=. NODE_OPTIONS='--import=tsx' next-rest-framework generate",
